{
  "expo": {
    "name": "yuting-app",
    "slug": "yuting-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/yuting_.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/yuting_png.png",
      "resizeMode": "native",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/yuting_.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/yuting_.png"
    }
  }
}
